language = Objective-C
package = native.ios.socketWrench
headers = SocketWrench/SocketWrench-Swift.h
headerFilter = SocketWrench/**

staticLibraries = libSocketWrench.a
libraryPaths.ios_x64 = ../SocketWrench/DerivedData/Build/Products/Release-iphonesimulator
libraryPaths.ios_arm64 = ../SocketWrench/DerivedData/Build/Products/Release-iphoneos

linkerOpts = -L/usr/lib/swift
linkerOpts.ios_x64 = -ios_simulator_version_min 11.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphonesimulator/
linkerOpts.ios_arm64 = -iphoneos_version_min 11.0.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphoneos/